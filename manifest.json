{
  "name": "Hidive caption hider",
  "description": "An extension to hide the captions on Hidive",
  "version": "1.0",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": ["*://www.hidive.com/*"],
      "css": [
        "hide.css"
      ],
      "run_at": "document_start"
    }
  ]
}
